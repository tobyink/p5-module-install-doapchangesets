	dcs:changeset [
		dcs:versus my:v_0-00_01 ;
		dcs:item
			[ rdfs:label "Don't recommend QNames containing '.' -- they're supported by libraptor, but illegal in Turtle."@en ]
	] .

my:v_0-00_03

	a               :Version ;
	dc:issued	"2010-01-11"^^xsd:date ;
	:revision	"0.00_03"^^xsd:string ;
